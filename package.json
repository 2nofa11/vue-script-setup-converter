{
  "name": "vue-script-setup-converter",
  "version": "0.0.2",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "bin": {
    "vue-script-setup-converter": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepack": "unbuild",
    "build:watch": "unbuild --stub",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:update": "vitest --update",
    "changelog": "changelogen --release",
    "release": "changelogen --release && npm publish"
  },
  "peerDependencies": {
    "ts-morph": ">=17.0.1",
    "typescript": ">=4.9.4"
  },
  "devDependencies": {
    "@types/prettier": "^2.7.1",
    "changelogen": "^0.6.1",
    "prettier": "^2.7.1",
    "typescript": "^5.7.2",
    "unbuild": "^2.0.0",
    "vitest": "2.1.9"
  },
  "dependencies": {
    "@vue/compiler-sfc": "3.2.40",
    "consola": "^3.4.2",
    "knitwork": "^1.1.0"
  }
}
